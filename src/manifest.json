{
    "name": "Slack Emoji Tools",
    "description": "Tools that make life as a Slack emoji addict a little easier.",
    "version": "1.0.1",
    "permissions": [
        "activeTab",
        "http://*/",
        "https://*/"
    ],
    "content_scripts": [{
        "matches": ["*://*.slack.com/*/emoji*"],
        "js": [
            "content.js"
        ],
        "run_at": "document_end"
    }],
    "icons": {
      "128": "images/icon_128.png"
    },
    "manifest_version": 2
}
